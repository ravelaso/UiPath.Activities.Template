{
  "$schema": "http://json.schemastore.org/template",
  "author": "Andres Ravelo",
  "classifications": ["Common", "Library", "UiPath"],
  "identity": "UiPath.MultiProjectTemplate",
  "name": "UiPath Template",
  "shortName": "uipath-template",
  "version": "1.0.1",
  "tags": {
    "language": "C#",
    "type": "solution"
  },
  "sourceName": "ProjectName",
  "preferNameDirectory": true,
  "guids": [
    "EE59ADC1-89E0-4F6C-935A-63F3A52D6D8D",
    "5B6E7870-9E3C-4D41-A39C-A91C4500B40D",
    "EA2F190B-3FB6-4D52-A532-07BFCC5A4C20",
    "3E6B7A05-3870-4F2E-AA26-E77C3FFFCC32"
  ],
  "symbols": {
    "ProjectName": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "CoolProject"
    },
    "TargetFramework": {
      "type": "parameter",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0",
          "description": "Target .NET 6.0"
        },
        {
          "choice": "net8.0",
          "description": "Target .NET 8.0"
        }
      ],
      "defaultValue": "net6.0",
      "description": "The target framework for the project."
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "condition": "(ProjectName)",
          "rename": {
            "UiPath.Activities.ProjectName": "ProjectName"
          }
        }
      ],
      "exclude": [".git/**"]
    }
  ],
  "primaryOutputs": [
    {
      "path": "ProjectName"
    },
    {
      "path": "ProjectName.Packaging"
    },
    {
      "path": "ProjectName.Tests"
    }
  ]
}
